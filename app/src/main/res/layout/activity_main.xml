        app:layout_constraintStart_toEndOf="@+id/tiktaktoe_btn1"
        app:layout_constraintTop_toBottomOf="@+id/tiktaktoe_btn5"
        app:srcCompat="@drawable/empty" />

    <ImageView
        android:id="@+id/tiktaktoe_btn4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="w,1:1"
        app:layout_constraintEnd_toStartOf="@+id/tiktaktoe_btn2"
        app:layout_constraintHorizontal_bias="0.0"
